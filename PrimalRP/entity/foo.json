{
  "format_version": "1.8.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "tutorial:foo",
      "materials": {
        "default": "foo"
      },
      "textures": {
        "default": "textures/primal/entity/foo/foo"
      },
      "geometry": {
        "default": "geometry.foo",
		"armor": "geometry.foo.armor"
      },
	  "scripts": {
        "pre_animation": [
          "variable.tcos0 = (Math.cos(global.anim_pos * 38.17) * global.anim_speed / variable.gliding_speed_value) * 57.3;",
          "variable.tcos1 = -variable.tcos0;"
        ]
	  },
      "animations": {
		"base": "animation.foo.base_pose",
        "look_at_target_default": "animation.foo.look_at_target.default",
        "look_at_target_gliding": "animation.foo.look_at_target.gliding",
        "look_at_target_swimming": "animation.foo.look_at_target.swimming",
        "move": "animation.foo.move",
        "holding": "animation.foo.holding",
        "bow_and_arrow": "animation.foo.bow_and_arrow"
      },
      "animation_controllers": [
		{ "move": "controller.animation.foo.move" },
        { "look_at_target": "controller.animation.foo.look_at_target" },
        { "holding": "controller.animation.foo.holding" },
        { "bow_and_arrow": "controller.animation.foo.bow_and_arrow" }
      ],
      "render_controllers": [ "controller.render.foo" ],
      "spawn_egg": {
        "base_color": "#87b5ff",
        "overlay_color": "#87b5ff"
      },
      "enable_attachables": true
    }
  }
}