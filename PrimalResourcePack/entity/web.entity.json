{
  "format_version": "1.12.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "primal:web",
      "materials": {
        "default": "entity_alphatest"
      },
      "textures": {
        "default": "textures/entity/web"
      },
      "geometry": {
        "default": "geometry.web"
      },
      "animations": {
        "move": "animation.web.move"
      },
      "scripts": {
        "pre_animation": [
          "variable.shake = query.shake_time - query.frame_alpha;",
          "variable.shake_power = variable.shake > 0.0 ? -Math.sin(variable.shake * 200.0) * variable.shake : 0.0;"
        ],
        "animate": [
          "move"
        ]
      },
      "render_controllers": [ "controller.render.web" ]
    }
  }
}